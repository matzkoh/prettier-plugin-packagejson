{
  "name": "prettier-plugin-packagejson",
  "version": "0.0.0-development",
  "description": "Prettier package.json plugin to make the order of properties nice.",
  "keywords": [
    "package.json",
    "plugin",
    "prettier"
  ],
  "repository": "github:matzkoh/prettier-plugin-packagejson",
  "license": "MIT",
  "main": "lib/index.cjs",
  "directories": {
    "lib": "lib"
  },
  "files": [
    "lib/**/*.js",
    "!**/*.snap",
    "!lib/**/*.test.js"
  ],
  "scripts": {
    "fix": "run-p fix:*",
    "fix:prettier": "npm run lint:prettier -- --write",
    "lint": "run-p lint:*",
    "lint:prettier": "prettier -c .",
    "test": "run-s lint test:*",
    "test:unit": "cross-env NODE_OPTIONS=--experimental-vm-modules jest"
  },
  "dependencies": {
    "sort-package-json": "3.4.0",
    "synckit": "0.11.11"
  },
  "devDependencies": {
    "cross-env": "7.0.3",
    "jest": "30.2.0",
    "npm-run-all2": "8.0.4",
    "prettier": "3.6.2"
  },
  "peerDependencies": {
    "prettier": ">= 1.16.0"
  },
  "peerDependenciesMeta": {
    "prettier": {
      "optional": true
    }
  }
}
